{
  "name": "way_farer",
  "version": "1.0.0",
  "description": "WayFarer is a public bus transportation booking API, that would be used to power front-end apps.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=develop npm run setupdb && cross-env NODE_ENV=develop nodemon index.js --exec babel-node",
    "test": "cross-env NODE_ENV=test nyc --babel-cache mocha test/ --exit",
    "lint": "eslint --fix ./**/*.js",
    "lint-md": "remark . --output",
    "setupdb": "babel-node database/setupDB.js",
    "dropdb": "cross-env NODE_ENV=develop babel-node database/dropDB.js",
    "seedDb": "babel-node database/seedDB.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Wokoro/way_farer.git"
  },
  "keywords": [
    "Wayfarer",
    "Server",
    "Front-end"
  ],
  "author": "wokorosamuel@yahoo.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wokoro/way_farer/issues"
  },
  "homepage": "https://github.com/Wokoro/way_farer#readme",
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.5.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "nyc": "^14.1.1",
    "coveralls": "^3.0.4"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.5.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^1.19.1",
    "pg": "^7.11.0",
    "redis": "^2.8.0",
    "remark-cli": "^6.0.1",
    "remark-lint": "^6.0.5",
    "remark-preset-lint-recommended": "^3.0.3",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.6",
    "winston": "^3.2.1",
    "snyk": "^1.192.6"
  },
  "snyk": true
}
